@model IEnumerable<Projek_Restoran.Models.JenisPesanan>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NamaJenisPesanan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaJenisPesanan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdJenisPesanan">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdJenisPesanan">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdJenisPesanan">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
