@model Projek_Restoran.Models.Meja

@{
    ViewData["Title"] = "Delete";
}

<h2>Hapus</h2>

<h3>Apakah Kamu Yakin Ingin Menghapus ini ?</h3>
<div>
    <h4>Meja</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("No Meja")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NomorMeja)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdMeja" />
        <input type="submit" value="Hapus" class="btn btn-danger" /> |
        <a asp-action="Index">Kembali</a>
    </form>
</div>
