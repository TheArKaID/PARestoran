@model Projek_Restoran.Models.Kategori

@{
    ViewData["Title"] = "Delete";
}

<h2>Hapus</h2>

<h3>Apakah Kamu Yakin Ingin Menghapus ini ?</h3>
<div>
    <h4>Kategori</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Nama Kategori")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NamaKategori)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdKategori" />
        <input type="submit" value="Hapus" class="btn btn-primary" /> |
        <a asp-action="Index">Kembali</a>
    </form>
</div>
